config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "BGM_SE_Player",
"language": "GDScript",
"path": "res://Globals/Music.gd"
}, {
"base": "Control",
"class": "CardStack",
"language": "GDScript",
"path": "res://Nodes/card_stack/card_stack.gd"
}, {
"base": "Node",
"class": "TTTState",
"language": "GDScript",
"path": "res://Globals/TicTacToeBoard.gd"
}, {
"base": "Node",
"class": "TicTacToeGrid",
"language": "GDScript",
"path": "res://TicTacToe/TicTacToeGrid.gd"
} ]
_global_script_class_icons={
"BGM_SE_Player": "",
"CardStack": "",
"TTTState": "",
"TicTacToeGrid": ""
}

[application]

config/name="tic-tac-toe-21"
run/main_scene="res://Title/Main.tscn"

[autoload]

Transit="*res://addons/transit/Transit.tscn"
Deck="*res://Globals/Deck.gd"
gVar="*res://Globals/gVar.gd"
Board="*res://Globals/TicTacToeBoard.tscn"
Music="*res://Globals/Music.tscn"

[display]

window/size/width=1080
window/size/height=1920
window/dpi/allow_hidpi=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray(  )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[input]

ui_touch={
"deadzone": 0.5,
"events": [ Object(InputEventMouseButton,"resource_local_to_scene":false,"resource_name":"","device":-1,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"button_mask":0,"position":Vector2( 0, 0 ),"global_position":Vector2( 0, 0 ),"factor":1.0,"button_index":1,"pressed":false,"doubleclick":false,"script":null)
 ]
}

[input_devices]

